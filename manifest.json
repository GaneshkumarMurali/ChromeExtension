{
    "name":"Report an Incident",
    "description": "Create a ServiceNow ticket",
    "version":"1.0",
    "manifest_version":2,
    "background":{
        "persistent":false,
        "scripts":["background.js"]
    },
    "browser_action":
    {
        "default_popup":"popup.html",
        "default_ico": "icon.png"
    },
    "permissions":[
        "activeTab",
        "tabs",
        "<all_urls>",
        "https://dev12176.service-now.com/",
        "identity",
        "identity.email"
    ]
}